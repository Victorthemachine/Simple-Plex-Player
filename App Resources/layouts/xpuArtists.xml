<?xml version="1.0" encoding="UTF-8"?>
<document>
   <listTemplate>
      {{#MediaContainer}}
      <list>
         <header>
            <title>{{title2}}</title>
         </header>
         <section>
           {{#Metadata}}
            <listItemLockup template="xpuOneArtist.xml" data="{{key}}" presentation="push" key="" docID="">
               <title>{{title}}</title>
               <relatedContent>
                  <lockup>
                     <img src="{{{thumb}}}" width="482" height="482" />
                     <title>{{title}}</title>
                     <description>{{summary}}</description>
                  </lockup>
               </relatedContent>
            </listItemLockup>
            {{/Metadata}}
         </section>
      </list>
      {{/MediaContainer}}
   </listTemplate>
</document>
